# RDF Schema for Industry: banking

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ind: <http://model.lakehouse.org/industry/banking#> .

# --- Base Class ---
ind:Industry rdf:type rdfs:Class ; rdfs:label 'Industry' .

# --- Industry Class ---
ind:Banking rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Industry ;
    rdfs:label 'banking' ;
    rdfs:comment 'covers both Retail banking and investment banking, covers everyday banking operations' .

# --- Domain Classes ---
ind:Customer rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Customer' ;
    rdfs:comment 'Core master data about individuals and organizations that hold accounts, loans, cards, and other banking relationships.' .

ind:Account rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Account' ;
    rdfs:comment 'Master data for bank accounts, representing the contractual relationship between a customer and the bank for deposits, loans, and other products.' .

ind:Transaction rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Transaction' ;
    rdfs:comment 'Transactional data capturing every movement of funds, including payments, transfers, fees, and related compliance events.' .

ind:Loan rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Loan' ;
    rdfs:comment 'Master data for loan products and the lifecycle of each loan agreement.' .

ind:Deposit rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Deposit' ;
    rdfs:comment 'Master data for deposit products such as savings, checking, and term deposits.' .

ind:Card rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Card' ;
    rdfs:comment 'Master data for debit, credit, and prepaid cards issued to customers, including limits, fees, and security features.' .

ind:Product rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Product' ;
    rdfs:comment 'Catalog of banking products such as loan types, deposit types, and card offerings, including pricing, features, eligibility, and compliance attributes.' .

ind:Branch rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Branch' ;
    rdfs:comment 'Physical and virtual branch locations delivering banking services, including staff, ATMs, and performance metrics.' .

ind:Risk rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Risk' ;
    rdfs:comment 'Enterprise risk management data covering credit, market, operational, and compliance risk, with models, scores, events, and policies.' .

ind:Compliance rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Compliance' ;
    rdfs:comment 'Regulatory compliance monitoring, issues, audits, and remediation across the bank.' .

ind:Payments rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Payments' ;
    rdfs:comment 'Payment initiation, methods, schedules, fees and related risk/compliance.' .

ind:Analytics rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Analytics' ;
    rdfs:comment 'Derived analytical data products for reporting, BI dashboards, and machine-learning models across banking.' .

ind:Employee rdf:type rdfs:Class ;
    rdfs:subClassOf ind:Banking ;
    rdfs:label 'Employee' ;
    rdfs:comment 'Master data for bank employees, their roles, compensation, performance, and compliance training.' .

